pe=\"password\"\r\n        v-model=\"password\"\r\n        placeholder=\"Password\"\r\n      />\r\n      <button type=\"submit\" class=\"form-btn neu-border\">Sign in</button>\r\n      <!-- <div class=\"form-social-login\">\r\n        <button class=\"form-btn neu-border form-social-btn\">\r\n          <i class=\"fab fa-google\"></i>\r\n        </button>\r\n        <button class=\"form-btn neu-border form-social-btn\">\r\n          <i class=\"fab fa-facebook-f\"></i>\r\n        </button>\r\n      </div> -->\r\n\r\n      <p>\r\n        Not a member?\r\n        <router-link :to=\"{ name: 'Register' }\">Create an account</router-link>\r\n      </p>\r\n    </form>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      fetch(\"https://fente.herokuapp.com/users/login\", {\r\n        method: \"POST\",\r\n        body: JSON.stringify({\r\n          email: this.email,\r\n          password: this.password,\r\n        }),\r\n        headers: {\r\n          \"Content-type\": \"application/json; charset=UTF-8\",\r\n        },\r\n      })\r\n        .then((response) => response.json())\r\n        .then((json) => {\r\n          if (json.jwt) {\r\n            localStorage.setItem(\"jwt\", json.jwt);\r\n            alert(\"User logged in\");\r\n            return this.$router.push({ name: \"Products\" });\r\n          }\r\n          alert(\"cannot login\");\r\n        })\r\n        .catch((err) => {\r\n          alert(err);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\nsection {\r\n  background-image: url(\"https://picsum.photos/1920/1080\");\r\n  background-size: cover;\r\n  height: 100vh;\r\n}\r\n\r\nform {\r\n  position: fixed;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\n}\r\n\r\n.neu-border {\r\n  border-radius: 30px;\r\n  background: #f5f5f56b;\r\n  box-shadow: 8px 8px 15px